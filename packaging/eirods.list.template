# =-=-=-=-=-=-=-
# preinstall
# =-=-=-=-=-=-=-
%preinstall << END_PREINSTALL

# determine whether this is an upgrade
if [ "$$1" -eq "$$1" ] 2>/dev/null ; then
  # integer, therefore rpm
  if [ $$1 -gt 1 ] ; then
    PACKAGEUPGRADE="true"
  else
    PACKAGEUPGRADE="false"
  fi
else
  # string, therefore deb
  if [ "$$1" = "upgrade" ] ; then
    PACKAGEUPGRADE="true"
  else
    PACKAGEUPGRADE="false"
  fi
fi

if [ "$$PACKAGEUPGRADE" = "true" ] ; then
  echo "Upgrading Existing E-iRODS Installation"
  mkdir -p $EIRODS_HOME_DIR
  touch $EIRODS_HOME_DIR/upgrade.tmp
else
  echo "Creating Service Group and Account: $OS_EIRODS_ACCT at $EIRODS_HOME_DIR "

  set +e
  CHECKGROUP=`getent group $OS_EIRODS_ACCT `
  CHECKACCT=`getent passwd $OS_EIRODS_ACCT `
  set -e
  if [ "$$CHECKGROUP" = "" ]; then
    /usr/sbin/groupadd -r $OS_EIRODS_ACCT
  else
    echo "ERROR :: Please delete the existing Service Group $OS_EIRODS_ACCT before proceeding."
    if [ "$$CHECKACCT" = "" ]; then
      echo "      :: The $OS_EIRODS_ACCT Service Account must be deleted prior to deleting the $OS_EIRODS_ACCT Service Group."
    fi
    exit 1
  fi

  if [ "$$CHECKACCT" = "" ]; then
    useradd -r -d $EIRODS_HOME_DIR -M -s /bin/bash -g $OS_EIRODS_ACCT -c "E-iRODS Administrator" $OS_EIRODS_ACCT
  else
    echo "ERROR :: Please delete the existing Service Account $OS_EIRODS_ACCT before proceeding."
    exit 1
  fi

  ACCT=$OS_EIRODS_ACCT
  mkdir -p $EIRODS_HOME_DIR
  chown $$ACCT:$$ACCT $EIRODS_HOME_DIR
fi
END_PREINSTALL







# =-=-=-=-=-=-=-
# post install
